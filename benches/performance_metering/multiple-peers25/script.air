(seq
  (seq
    (call "init_peer_id" ("" "init") [] data) ; ok = ["p0","p1","p2","p3","p4","p5","p6","p7","p8","p9","p10","p11","p12","p13","p14","p15","p16","p17","p18","p19","p20","p21","p22","p23","p24"]
    (fold data p
      (seq
        (fold data x
          (seq
            (fold data y
              (seq
                (call p ("serv" "args") [p x y] $stream) ; behaviour = echo
                (next y)))
            (next x)))
        (next p))))
  (seq
     (call "other_peer_id" ("" "collect") []) ; behaviour = unit
     (call "init_peer_id" ("" "complete") [])))
