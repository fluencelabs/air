(seq
  (seq
    (call "init_peer_id" ("" "init") [] data)
    (fold data x
    (seq
      (fold data y
        (par
          (call "init_peer_id" ("serv" "args") [x y] $stream) ; behaviour = echo
          (next y)))
      (next x))))
  (seq
     (call "other_peer_id" ("" "complete") [])
     (canon "init_peer_id" $stream #canon)))
